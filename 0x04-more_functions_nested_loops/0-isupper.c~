#include "main.h"
/**
 *_isupper: prints characters in uppercase
 *@c : prints the charcters
 *Return: 0 or 1 for success
 *On error, -1 is returned, and errno is set appropriately. 
 */
int _isupper(char c)
{
    if (c >= 'A' && c <= 'Z')
    {
        return (1);
    }
    else
    {
        return (0);
    }
}