SRC=/*
CC = gcc 
CFLAGS = -Wall -Wextra -Werror -pedantic -std=gnu89
LIBS = -lm
OBJ = main.o school.o

all: school

school: $(OBJ)
	$(CC) $(CFLAGS) -o school main.o school.o $(LIBS)

main.o: main.c school.c m.h
	$(CC) $(CFLAGS) -c main.c

school.o: school.c m.h
	$(CC) $(CFLAGS)  -c school.c

clean:
		rm -f *.o school
